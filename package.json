{
  "name": "@dlghq/dialog-bot-sdk",
  "version": "4.1.0",
  "description": "Dialog Bot SDK",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "predocs": "rm -rf docs/",
    "docs": "typedoc --excludePrivate --theme minimal --mode file --out docs src/index.ts",
    "postdocs": "buildbranch gh-pages docs",
    "prebuild": "rm -rf lib/",
    "build": "tsc",
    "preversion": "npm test",
    "postversion": "npm run release",
    "prerelease": "npm run build",
    "release": "git push --follow-tags",
    "postrelease": "npm publish"
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@dlghq/dialog-api": "0.9.0",
    "@grpc/proto-loader": "0.5.1",
    "bluebird": "3.5.5",
    "dataclass": "1.1.1",
    "grpc": "1.21.1",
    "jimp": "0.6.4",
    "lodash": "4.17.13",
    "mime": "2.4.4",
    "node-fetch": "2.6.0",
    "rxjs": "6.5.2",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.27",
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.15",
    "@types/long": "4.0.0",
    "@types/node-fetch": "2.3.7",
    "@types/uuid": "3.4.4",
    "buildbranch": "2.0.1",
    "dotenv": "8.0.0",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "nodemon": "1.19.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typedoc": "0.14.2",
    "typescript": "3.5.2"
